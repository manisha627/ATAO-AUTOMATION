{
  "name": "cypressperse",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js",
    "cy:report": "node ./cypress/report-generator.js",
    "cy:merge": "cucumber-json-merge --dir ./cypress/cucumber-json --out ./cypress/cucumber-json/merged-test-results.json",
    "cy:test": "cypress run --spec cypress/e2e/features/*.feature --headed --browser chrome",
    "cy:testtags": "cypress-tags run -e TAGS=\"@R3S\" --headed --browser chrome",
    "cypress:run": "npx cypress run --record --key 07349327-fb8e-4a46-811d-26037de4cf1c",
    "cypress:runChromeHeadless": "npx cypress run --headless --browser chrome --record --key 07349327-fb8e-4a46-811d-26037de4cf1c"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cucumber-json-merge": "^0.0.8",
    "cypress": "^10.3.0",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-fill-command": "^1.0.2",
    "cypress-if": "^1.10.3",
    "cypress-iframe": "^1.0.1",
    "cypress-plugin-api": "^2.11.0",
    "cypress-real-events": "^1.7.6",
    "cypress-xpath": "^1.6.2",
    "multiple-cucumber-html-reporter": "^1.20.0"
  },
  "devDependencies": {
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-failed-log": "^2.10.0",
    "typescript": "^5.1.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/e2e",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json"
    }
  }
}